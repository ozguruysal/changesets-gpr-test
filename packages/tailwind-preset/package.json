{
  "name": "@ozguruysal/spectre-tailwind-preset",
  "version": "0.1.0-alpha.2",
  "description": "Tailwind preset for Spectre Design System",
  "private": false,
  "author": "Özgür Uysal",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./tokens.css": {
      "import": "./dist/tokens.css",
      "require": "./dist/tokens.css"
    }
  },
  "license": "ISC",
  "scripts": {
    "build": "tsup index.ts --format cjs,esm --dts --publicDir"
  },
  "devDependencies": {
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsup": "^8.0.2"
  },
  "peerDependencies": {
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1"
  }
}
